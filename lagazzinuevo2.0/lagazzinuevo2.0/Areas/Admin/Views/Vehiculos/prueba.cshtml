@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link rel="stylesheet" href="~/css/registro-trabajadores.css">


<div class="container">
    <div class="row">
        <div class="col-12 titulo"><h1>Registros Vehículos</h1></div>
        <div class="col-12  botones">
            <a class="btn-crear"  asp-action="CreateVehiculo"><i style="margin-right: 20px !important;" class="fas fa-plus-circle"></i>Registrar Vehículo</a>
            <a class="btn-crear" asp-controller="TiposVehiculos" asp-action="CreateTipoVehiculo"><i style="margin-right: 20px !important;" class="fas fa-plus-circle"></i>Registrar tipo Vehículo</a>

        </div>
    </div>
    <table id="tblVehiculo" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Marca</th>
                <th>Modelo</th>
                <th>Patente</th>
                <th>Año</th>
                <th>Tipo</th>
                <th colspan="1"></th>
            </tr>
        </thead>


    </table>

    @section Scripts{
        <script src="~/js/vehiculo.js"></script>
    }





</div>





