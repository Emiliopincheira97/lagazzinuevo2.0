@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="container">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Lista de Vehiculo</h2>
        </div>
        <div class="col-6">
            <a asp-action="CreateTipoVehiculo" class="btn btn-primary"><i class="fas fa-plus"></i>Ingresar Vehiculo</a>
        </div>
    </div>
</div>

<br />
<br />
<table id="tblVehiculo" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th colspan="1">Id</th>
            <th colspan="1">Marca</th>
            <th colspan="1">Modelo</th>
            <th colspan="1">Patente</th>
            <th colspan="1">Año</th>
            <th colspan="1">Tipo</th>
            <th colspan="1"></th>
        </tr>
    </thead>


</table>

@section Scripts{
    <script src="~/js/vehiculo.js"></script>
}